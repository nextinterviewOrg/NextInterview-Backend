{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/cron/removeRestriction",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/notification/send/dailyNotification",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/notification/send/weeklyNotification",
      "schedule": "0 0 * * *"
    }
  ]
}
